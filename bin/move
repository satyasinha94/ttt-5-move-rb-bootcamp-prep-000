#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," ",]
puts "Where would you like to go?"
input = gets.strip
def input_to_index(input)
  index = "#{input}".to_i - 1
end

def move(board, index, value = "X")
  board[index] = value
end
